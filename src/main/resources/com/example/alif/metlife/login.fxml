<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="456.0" prefWidth="772.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alif.metlife.LoginController">
   <children>
      <Label layoutX="247.0" layoutY="84.0" text="Sign in to your account" textFill="#0f611d">
         <font>
            <Font name="Songti TC Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="132.0" layoutY="129.0" text="Please enter your username and password to login" textFill="#0a3612">
         <font>
            <Font name="Songti TC Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="326.0" layoutY="32.0" text="MetLife" textFill="#961869">
         <font>
            <Font name="Songti TC Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" alignment="CENTER" layoutX="276.0" layoutY="190.0" prefHeight="38.0" prefWidth="221.0" promptText="USERNAME">
         <font>
            <Font name="Shobhika Bold" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTextField" alignment="CENTER" layoutX="278.0" layoutY="243.0" prefHeight="38.0" prefWidth="221.0" promptText="PASSWORD">
         <font>
            <Font name="Shobhika Bold" size="14.0" />
         </font>
      </TextField>
      <Button layoutX="326.0" layoutY="294.0" mnemonicParsing="false" onAction="#loginButtonOnAction" prefHeight="31.0" prefWidth="126.0" style="-fx-border-color: green;" text="LOGIN" />
      <Label fx:id="messageLabel" layoutX="135.0" layoutY="381.0" prefHeight="40.0" prefWidth="509.0" textFill="#0a3612">
         <font>
            <Font name="Songti TC Bold" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
