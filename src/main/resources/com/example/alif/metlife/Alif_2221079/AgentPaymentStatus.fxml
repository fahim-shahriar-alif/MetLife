<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="609.0" prefWidth="961.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alif.metlife.Alif_2221079.AgentPaymentStatusController">
   <children>
      <Label layoutX="360.0" layoutY="34.0" text="Monitor Payment Status">
         <font>
            <Font name="Courier New Bold" size="31.0" />
         </font>
      </Label>

      <TableView fx:id="paymentsTableView" layoutX="107.0" layoutY="100.0" prefHeight="400.0" prefWidth="750.0">
         <columns>
            <TableColumn fx:id="clientIdColumn" prefWidth="100.0" text="Client ID" />
            <TableColumn fx:id="clientNameColumn" prefWidth="150.0" text="Client Name" />
            <TableColumn fx:id="policyTypeColumn" prefWidth="150.0" text="Policy Type" />
            <TableColumn fx:id="dueAmountColumn" prefWidth="150.0" text="Due Amount" />
            <TableColumn fx:id="dueDateColumn" prefWidth="100.0" text="Due Date" />
            <TableColumn fx:id="statusColumn" prefWidth="100.0" text="Status" />
         </columns>
      </TableView>

      <HBox layoutX="107.0" layoutY="520.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#searchPaymentButtonOnAction" prefHeight="39.0" prefWidth="150.0" style="-fx-border-color: purple;" text="Search Payment">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#updateStatusButtonOnAction" prefHeight="39.0" prefWidth="150.0" style="-fx-border-color: purple;" text="Update Status">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="39.0" prefWidth="150.0" style="-fx-border-color: purple;" text="Back to Dashboard">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane> 